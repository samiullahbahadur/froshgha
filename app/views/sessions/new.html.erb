<div class="flex flex-col items-center ">
        <h1> Login Form</h1>
            <div>
              <%= form_with session:, url: sign_in_path do |form| %>
                </div>
            <div class="mb-3">
             <%= form.text_field :email,  placeholder:"Enter email",  autofocus:'true'%>
            </div>
            <div class="mb-3">
             <%= form.password_field :passwor, placeholder:"Enter password",  autocomplete:'off'   %>

            </div>
            <div class="mb-3">
             <%= form.submit 'Login' %>
            </div>
        <% end %>
</div>